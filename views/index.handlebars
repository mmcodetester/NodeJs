
<div class="row">
     {{#each results}}
     <div class="col-md-4">
        <div class="card">
            <img class="card-img-top" src="/static/uploads/{{image}}" alt="Card image cap">
            <div class="card-body">
                <h6 class="card-title mb-3">{{title}}</h6>
                <p class="card-text">{{body}}</p>
            </div>
        </div>
    </div>
    {{/each}}  

</div>
 